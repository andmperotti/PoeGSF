    <h1>Other Items Requested</h1>

    <img src="images/other.gif">

    <p class="centered-description">Click on the item name to complete it and move down list, click again to uncomplete it and move to top of list.</p>

        <ul class="all-requested">
            <li class="field-description">What thing - Account Name - Discord Name</li>
            <% others.forEach( el => { %>
                    <li class='requestedItem' data-id='<%=el._id%>'>
                        <span class='<%= el.completed === true ? 'completed' : 'not'%>'>
                            <%= el.requestedItem %></span> - <a href="https://www.pathofexile.com/account/view-profile/<%=el.userName%>"><%=el.userName %></a> - <%= el.discordName%>
                        
                    </li>
            <% }) %>    
        </ul>






    <script src="js/main.js"></script>
</body>
